{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "octo",
  "version": "0.1.2",
  "identifier": "app.octo",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://127.0.0.1:8888",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "echo 'skipping web build' || pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Octo",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "url": "docs/new"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "macOS": {
    },
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "mainBinaryName": "octo",
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/davidmyersdev/octo/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEJEODg5MkRDQjA0RUE3M0UKUldRK3AwNnczSktJdlNMcW1UZUE1dWpJUitQZDRmTG9QbXEyZitobC9OenBobHhjeXp0cFE1OTYK"
    }
  }
}
